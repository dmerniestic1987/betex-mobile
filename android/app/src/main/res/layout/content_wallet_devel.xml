<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="ar.com.betex.betexmobile.activities.WalletDevelActivity"
    tools:showIn="@layout/activity_wallet_devel"
    android:orientation="vertical">

    <TextView
        android:id="@+id/versionView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Button" />

    <EditText
        android:id="@+id/restulTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine" />

    <Button
        android:id="@+id/holaMundo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hola" />

    <Button
        android:id="@+id/placeBet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Put Bet"
        app:layout_constraintStart_toEndOf="@+id/versionView" />


</LinearLayout>